{
    "name": "blood72/laravel-riot-eloquent",
    "type": "library",
    "description": "Riot Eloquent for Laravel",
    "license": "MIT",
    "keywords": [
        "Laravel",
        "Riot Games",
        "Eloquent"
    ],
    "authors": [
        {
            "name": "blood72",
            "email": "blood72@naver.com"
        }
    ],
    "support": {
        "issues": "https://github.com/blood72/laravel-riot-eloquent/issues",
        "source": "https://github.com/blood72/laravel-riot-eloquent"
    },
    "suggest": {
        "blood72/laravel-riot-api": "This is Riot API package for Laravel."
    },
    "require": {
        "php": ">=7.3",
        "illuminate/database": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Blood72\\Riot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blood72\\Riot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blood72\\Riot\\RiotEloquentServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "php-cs-fixer fix .",
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
